<Window x:Class="DnD_5e_CharacterSheet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DnD_5e_CharacterSheet"
        xmlns:sys="clr-namespace:System;assembly=System"
        xmlns:controls="clr-namespace:DnD_5e_CharacterSheet.CustomControls"
        mc:Ignorable="d"
        Title="Character Sheet" MinHeight="1020" Height="1020" MinWidth="740" Width="740">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <Style TargetType="{x:Type Button}" x:Key="ImageButton">
            <Setter Property="Background" Value="#00ffffff" />
            <Setter Property="BorderBrush" Value="#00ffffff" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
        </Style>
        <Style TargetType="{x:Type Image}">
            <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
        </Style>
    </Window.Resources>
    <Window.InputBindings>
        <KeyBinding Key="N" Modifiers="Ctrl" Command="{Binding NewCommand}" />
        <KeyBinding Key="O" Modifiers="Ctrl" Command="{Binding LoadCommand}" />
        <KeyBinding Key="S" Modifiers="Ctrl" Command="{Binding SaveCommand}" CommandParameter="{Binding LoadedFile}" />
        <KeyBinding Key="S" Modifiers="Ctrl+Shift" Command="{Binding SaveCommand}" CommandParameter="{x:Null}" />
        <KeyBinding Key="F4" Modifiers="Alt" Command="{Binding CloseCommand}" />
    </Window.InputBindings>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Menu Margin="0,0,0,5">
            <MenuItem Header="File">
                <MenuItem Header="New" Command="{Binding NewCommand}" InputGestureText="Ctrl+N" />
                <MenuItem Header="Open" Command="{Binding LoadCommand}" InputGestureText="Ctrl+O" />
                <Separator />
                <MenuItem Header="Save" Command="{Binding SaveCommand}" CommandParameter="{Binding LoadedFile}" InputGestureText="Ctrl+S" />
                <MenuItem Header="Save As" Command="{Binding SaveCommand}" CommandParameter="{x:Null}" InputGestureText="Ctrl+Shift+S" />
                <Separator />
                <MenuItem Header="Exit" Command="{Binding CloseCommand}" InputGestureText="Alt+F4" />
            </MenuItem>
        </Menu>
        <TabControl Grid.Row="1">
            <TabItem Header="Character Sheet">
                <Grid>
                    <ContentPresenter Height="941" Width="720" VerticalAlignment="Top" HorizontalAlignment="Left" Content="{StaticResource CharacterSheetBackground}" />
                    <Grid>
                        <Grid.Resources>
                            <Style TargetType="TextBox">
                                <Setter Property="Background" Value="#00ffffff" />
                                <Setter Property="BorderBrush" Value="#00ffffff" />
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                            </Style>
                            <Style TargetType="ComboBox">
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                                <Setter Property="Template" Value="{StaticResource ComboBoxControlTemplate}" />
                            </Style>
                        </Grid.Resources>
                        <TextBox Text="{Binding CharacterName, UpdateSourceTrigger=PropertyChanged}" FontSize="19" Width="215" Margin="42,68,0,0" />
                        <TextBox Text="{Binding ClassAndLevel, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="315,55,0,0" />
                        <TextBox Text="{Binding Background, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="450,55,0,0" />
                        <TextBox Text="{Binding PlayerName, UpdateSourceTrigger=PropertyChanged}" Width="91" Margin="585,55,0,0" />
                        <ComboBox SelectedValue="{Binding Race}" ItemsSource="{Binding Races}" Width="130" Margin="312,84,0,0"/>
                        <ComboBox SelectedValue="{Binding Alignment}" ItemsSource="{Binding Alignments}" Width="130" Margin="447,84,0,0" />
                        <TextBox Text="{Binding ExperiencePoints, UpdateSourceTrigger=PropertyChanged}" TextAlignment="Center" Width="91" Margin="585,86,0,0" />
                        <Grid>
                            <Grid.Resources>
                                <Style TargetType="TextBox">
                                    <Setter Property="Background" Value="#00ffffff" />
                                    <Setter Property="BorderBrush" Value="#00ffffff" />
                                    <Setter Property="HorizontalAlignment" Value="Left" />
                                    <Setter Property="VerticalAlignment" Value="Top" />
                                    <Setter Property="TextAlignment" Value="Center" />
                                    <Setter Property="FontSize" Value="12" />
                                </Style>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Left" />
                                    <Setter Property="VerticalAlignment" Value="Top" />
                                    <Setter Property="TextAlignment" Value="Center" />
                                    <Setter Property="FontSize" Value="18" />
                                </Style>
                            </Grid.Resources>
                            <StackPanel Margin="25,164,0,0">
                                <StackPanel.Resources>
                                    <Style TargetType="{x:Type controls:AbilityScore}">
                                        <Setter Property="Margin" Value="0,0,0,5" />
                                    </Style>
                                </StackPanel.Resources>
                                <controls:AbilityScore Score="{Binding Strength}" Ability="STRENGTH" />
                                <controls:AbilityScore Score="{Binding Dexterity}" Ability="DEXTERITY" SnapsToDevicePixels="True" />
                                <controls:AbilityScore Score="{Binding Constitution}" Ability="CONSTITUTION" />
                                <controls:AbilityScore Score="{Binding Intelligence}" Ability="INTELLIGENCE" />
                                <controls:AbilityScore Score="{Binding Wisdom}" Ability="WISDOM" />
                                <controls:AbilityScore Score="{Binding Charisma}" Ability="CHARISMA" />
                            </StackPanel>
                            <TextBox Text="{Binding ProficiencyBonus, UpdateSourceTrigger=PropertyChanged, StringFormat='{}{0:+0;-#}'}" Width="28" Margin="107,200,0,0" />
                            <TextBlock Text="{Binding PassiveWisdom}" Width="20" Margin="31,708,0,0" />
                            <TextBox Text="{Binding ArmorClass, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Width="30" Margin="274,168,0,0" />

                            <GroupBox Header="INITIATIVE" Margin="325,158,0,0" Style="{StaticResource OrnateBox2}" VerticalAlignment="Top" HorizontalAlignment="Left">
                                <TextBlock Text="{Binding Initiative, StringFormat='{}{0:+0;-#}'}" Width="33" Margin="0,3"/>
                            </GroupBox>
                            <GroupBox Header="SPEED" Margin="396,158,0,0" Style="{StaticResource OrnateBox2}" VerticalAlignment="Top" HorizontalAlignment="Left">
                                <TextBox Text="{Binding Speed, UpdateSourceTrigger=PropertyChanged}" Width="33" FontSize="18" Margin="0,2"/>
                            </GroupBox>
                            
                            <TextBox Text="{Binding HitPointMaximum, UpdateSourceTrigger=PropertyChanged}" Width="109" Margin="341,233,0,0" FontSize="8" />
                            <TextBox Text="{Binding CurrentHitPoints, UpdateSourceTrigger=PropertyChanged}" Width="100" Margin="308,258,0,0" />
                            <TextBox Text="{Binding TemporaryHitPoints, UpdateSourceTrigger=PropertyChanged}" Width="100" Margin="308,320,0,0" />

                            <Grid Margin="260,375,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                <Grid.Resources>
                                    <Style TargetType="GroupBox" BasedOn="{StaticResource OrnateBox2}">
                                        <Setter Property="HorizontalAlignment" Value="Left" />
                                        <Setter Property="VerticalAlignment" Value="Top" />
                                    </Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="FontSize" Value="7" />
                                    </Style>
                                    <Style TargetType="TextBox">
                                        <Setter Property="FontSize" Value="7" />
                                        <Setter Property="TextAlignment" Value="Center" />
                                        <Setter Property="BorderThickness" Value="0,0,0,1" />
                                    </Style>
                                </Grid.Resources>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <GroupBox>
                                    <GroupBox.Header>
                                        <TextBlock Text="HIT DICE" Margin="0,0,0,-2"/>
                                    </GroupBox.Header>
                                    <StackPanel VerticalAlignment="Top" Orientation="Horizontal" Margin="-7,-9,-7,29">
                                        <TextBlock Text="Total " />
                                        <TextBox Text="{Binding HitDice, UpdateSourceTrigger=PropertyChanged}" Width="62"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Column="2" >
                                    <GroupBox.Header>
                                        <TextBlock Text="DEATH SAVES" Margin="0,0,0,-2"/>
                                    </GroupBox.Header>
                                    <Grid VerticalAlignment="Top" HorizontalAlignment="Stretch" Margin="-8,-5,0,0">
                                        <Grid.Resources>
                                            <Style TargetType="{x:Type controls:TripleCheckBox}">
                                                <Setter Property="Margin" Value="3,0,-6,7" />
                                            </Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="TextAlignment" Value="Right" />
                                                <Setter Property="FontSize" Value="7" />
                                            </Style>
                                        </Grid.Resources>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="auto" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="SUCCESSES" Grid.Row="0" />
                                        <TextBlock Text="FAILURES" Grid.Row="1" />
                                        <controls:TripleCheckBox Grid.Row="0" Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Left" IsChecked1="{Binding DeathSaveSuccess1}" IsChecked2="{Binding DeathSaveSuccess2}" IsChecked3="{Binding DeathSaveSuccess3}"/>
                                        <controls:TripleCheckBox Grid.Row="1" Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Left" IsChecked1="{Binding DeathSaveFailure1}" IsChecked2="{Binding DeathSaveFailure2}" IsChecked3="{Binding DeathSaveFailure3}" Margin="3,0,-6,6"/>
                                    </Grid>
                                </GroupBox>
                            </Grid>

                            <StackPanel Margin="100,236,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                <StackPanel.Resources>
                                    <Style TargetType="GroupBox" BasedOn="{StaticResource OrnateBox1}">
                                        <Setter Property="HorizontalAlignment" Value="Left" />
                                        <Setter Property="VerticalAlignment" Value="Top" />
                                        <Setter Property="Margin" Value="0,0,0,8" />
                                    </Style>
                                </StackPanel.Resources>
                                <GroupBox Header="SAVING THROWS">
                                    <StackPanel Margin="2,0,42,8">
                                        <controls:ThrowModifier Label="Strength" IsChecked="{Binding StrengthSavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding StrengthSavingThrow, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Dexterity" IsChecked="{Binding DexteritySavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding DexteritySavingThrow, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Constitution" IsChecked="{Binding ConstitutionSavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding ConstitutionSavingThrow, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Intelligence" IsChecked="{Binding IntelligenceSavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding IntelligenceSavingThrow, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Wisdom" IsChecked="{Binding WisdomSavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding WisdomSavingThrow, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Charisma" IsChecked="{Binding CharismaSavingThrowEnabled, Mode=TwoWay}" ModifierValue="{Binding CharismaSavingThrow, Mode=OneWay}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Header="SKILLS">
                                    <StackPanel Margin="2,0,6,18">
                                        <controls:ThrowModifier Label="Acrobatics (Dex)" IsChecked="{Binding AcrobaticsEnabled, Mode=TwoWay}" ModifierValue="{Binding Acrobatics, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Animal Handling (Wis)" IsChecked="{Binding AnimalHandlingEnabled, Mode=TwoWay}" ModifierValue="{Binding AnimalHandling, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Arcana (Int)" IsChecked="{Binding ArcanaEnabled, Mode=TwoWay}" ModifierValue="{Binding Arcana, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Athletics (Str)" IsChecked="{Binding AthleticsEnabled, Mode=TwoWay}" ModifierValue="{Binding Athletics, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Deception (Cha)" IsChecked="{Binding DeceptionEnabled, Mode=TwoWay}" ModifierValue="{Binding Deception, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="History (Int)" IsChecked="{Binding HistoryEnabled, Mode=TwoWay}" ModifierValue="{Binding History, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Insight (Wis)" IsChecked="{Binding InsightEnabled, Mode=TwoWay}" ModifierValue="{Binding Insight, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Intimidation (Cha)" IsChecked="{Binding IntimidationEnabled, Mode=TwoWay}" ModifierValue="{Binding Intimidation, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Investigation (Int)" IsChecked="{Binding InvestigationEnabled, Mode=TwoWay}" ModifierValue="{Binding Investigation, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Medicine (Wis)" IsChecked="{Binding MedicineEnabled, Mode=TwoWay}" ModifierValue="{Binding Medicine, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Nature (Int)" IsChecked="{Binding NatureEnabled, Mode=TwoWay}" ModifierValue="{Binding Nature, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Perception (Wis)" IsChecked="{Binding PerceptionEnabled, Mode=TwoWay}" ModifierValue="{Binding Perception, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Performance (Cha)" IsChecked="{Binding PerformanceEnabled, Mode=TwoWay}" ModifierValue="{Binding Performance, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Persuasion (Cha)" IsChecked="{Binding PersuasionEnabled, Mode=TwoWay}" ModifierValue="{Binding Persuasion, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Religion (Int)" IsChecked="{Binding ReligionEnabled, Mode=TwoWay}" ModifierValue="{Binding Religion, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Sleight of Hand (Dex)" IsChecked="{Binding SleightOfHandEnabled, Mode=TwoWay}" ModifierValue="{Binding SleightOfHand, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Stealth (Dex)" IsChecked="{Binding StealthEnabled, Mode=TwoWay}" ModifierValue="{Binding Stealth, Mode=OneWay}" />
                                        <controls:ThrowModifier Label="Survival (Wis)" IsChecked="{Binding SurvivalEnabled, Mode=TwoWay}" ModifierValue="{Binding Survival, Mode=OneWay}" />
                                    </StackPanel>
                                </GroupBox>
                            </StackPanel>
                            <Grid Margin="259,457,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                <Grid.Resources>
                                    <Style TargetType="TextBox" BasedOn="{StaticResource AngledCornerTextBox}">
                                        <Setter Property="Margin" Value="0,5,0,0" />
                                        <Setter Property="HorizontalAlignment" Value="Stretch" />
                                    </Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="FontSize" Value="7" />
                                    </Style>
                                </Grid.Resources>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="77" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="37" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="75" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0">
                                    <TextBlock Text="NAME" />
                                    <TextBox Text="{Binding Weapon1Name, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon2Name, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon3Name, UpdateSourceTrigger=PropertyChanged}" />
                                </StackPanel>
                                <StackPanel Grid.Column="2">
                                    <TextBlock Text="ATK BONUS" />
                                    <TextBox Text="{Binding Weapon1AtkBonus, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon2AtkBonus, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon3AtkBonus, UpdateSourceTrigger=PropertyChanged}" />
                                </StackPanel>
                                <StackPanel Grid.Column="4">
                                    <TextBlock Text="DAMAGE/TYPE" />
                                    <TextBox Text="{Binding Weapon1DamageType, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon2DamageType, UpdateSourceTrigger=PropertyChanged}" />
                                    <TextBox Text="{Binding Weapon3DamageType, UpdateSourceTrigger=PropertyChanged}" />
                                </StackPanel>
                            </Grid>
                            
                            <TextBox Text="{Binding CopperPieces, UpdateSourceTrigger=PropertyChanged}" Margin="270,721,0,0" Width="30" />
                            <TextBox Text="{Binding SilverPieces, UpdateSourceTrigger=PropertyChanged}" Margin="270,752,0,0" Width="30" />
                            <TextBox Text="{Binding ElectrumPieces, UpdateSourceTrigger=PropertyChanged}" Margin="270,783,0,0" Width="30" />
                            <TextBox Text="{Binding GoldPieces, UpdateSourceTrigger=PropertyChanged}" Margin="270,815,0,0" Width="30" />
                            <TextBox Text="{Binding PlatinumPieces, UpdateSourceTrigger=PropertyChanged}" Margin="270,846,0,0" Width="30" />
                            <Grid>
                                <Grid.Resources>
                                    <Style TargetType="TextBox">
                                        <Setter Property="Padding" Value="5,5,5,0" />
                                        <Setter Property="TextBlock.LineHeight" Value="12" />
                                        <Setter Property="TextBlock.LineStackingStrategy" Value="BlockLineHeight" />
                                        <Setter Property="Background">
                                            <Setter.Value>
                                                <DrawingBrush TileMode="Tile" Viewbox="0,0.5,1,1" Stretch="None" Viewport="0,0,12,12" ViewportUnits="Absolute">
                                                    <DrawingBrush.Drawing>
                                                        <GeometryDrawing>
                                                            <GeometryDrawing.Pen>
                                                                <Pen Brush="Gray" Thickness="0.25"/>
                                                            </GeometryDrawing.Pen>
                                                            <GeometryDrawing.Geometry>
                                                                <LineGeometry StartPoint="0,0" EndPoint="12,0"/>
                                                            </GeometryDrawing.Geometry>
                                                        </GeometryDrawing>
                                                    </DrawingBrush.Drawing>
                                                </DrawingBrush>
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="BorderBrush" Value="#00ffffff" />
                                        <Setter Property="HorizontalAlignment" Value="Left" />
                                        <Setter Property="VerticalAlignment" Value="Top" />
                                        <Setter Property="AcceptsReturn" Value="True" />
                                        <Setter Property="TextWrapping" Value="Wrap" />
                                        <Setter Property="TextAlignment" Value="Left" />
                                        <Setter Property="FontSize" Value="10" />
                                        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
                                    </Style>
                                </Grid.Resources>
                                <TextBox Text="{Binding OtherProficienciesAndLanguages, UpdateSourceTrigger=PropertyChanged}" Width="210" Height="160" Margin="26,750,0,0" MaxLines="12" />
                                <TextBox Text="{Binding WeaponNotes, UpdateSourceTrigger=PropertyChanged}" Width="214" Height="145" Margin="253,537,0,0" MaxLines="12" />
                                <TextBox Text="{Binding Equipment, UpdateSourceTrigger=PropertyChanged}" Width="155" Height="200" Margin="310,708,0,0" />
                                <TextBox Text="{Binding FeaturesAndTraits, UpdateSourceTrigger=PropertyChanged}" Width="214" Height="450" Margin="481,457,0,0" />
                            </Grid>
                            <Grid>
                                <Grid.Resources>
                                    <Style TargetType="TextBox">
                                        <Setter Property="Padding" Value="5,6,5,0" />
                                        <Setter Property="TextBlock.LineHeight" Value="13" />
                                        <Setter Property="TextBlock.LineStackingStrategy" Value="BlockLineHeight" />
                                        <Setter Property="Background">
                                            <Setter.Value>
                                                <DrawingBrush TileMode="Tile" Viewbox="0,0.5,1,1" Stretch="None" Viewport="0,0,13,13" ViewportUnits="Absolute">
                                                    <DrawingBrush.Drawing>
                                                        <GeometryDrawing>
                                                            <GeometryDrawing.Pen>
                                                                <Pen Brush="Gray" Thickness="0.25"/>
                                                            </GeometryDrawing.Pen>
                                                            <GeometryDrawing.Geometry>
                                                                <LineGeometry StartPoint="0,0" EndPoint="13,0"/>
                                                            </GeometryDrawing.Geometry>
                                                        </GeometryDrawing>
                                                    </DrawingBrush.Drawing>
                                                </DrawingBrush>
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="BorderBrush" Value="#00ffffff" />
                                        <Setter Property="HorizontalAlignment" Value="Left" />
                                        <Setter Property="VerticalAlignment" Value="Top" />
                                        <Setter Property="AcceptsReturn" Value="True" />
                                        <Setter Property="TextWrapping" Value="Wrap" />
                                        <Setter Property="TextAlignment" Value="Left" />
                                        <Setter Property="FontSize" Value="11" />
                                        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />
                                    </Style>
                                </Grid.Resources>
                                <TextBox Text="{Binding PersonalityTraits, UpdateSourceTrigger=PropertyChanged}" Width="196" Height="55" Margin="490,169,0,0" />
                                <TextBox Text="{Binding Ideals, UpdateSourceTrigger=PropertyChanged}" Width="196" Height="47" Margin="490,245,0,0" />
                                <TextBox Text="{Binding Bonds, UpdateSourceTrigger=PropertyChanged}" Width="196" Height="47" Margin="490,313,0,0" />
                                <TextBox Text="{Binding Flaws, UpdateSourceTrigger=PropertyChanged}" Width="196" Height="47" Margin="490,379,0,0" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Background Info">
                <Grid>
                    <ContentPresenter Height="941" Width="720" VerticalAlignment="Top" HorizontalAlignment="Left" Content="{StaticResource BackgroundInfoBackground}" />
                    <Grid>
                        <Grid.Resources>
                            <Style TargetType="TextBox">
                                <Setter Property="Background" Value="#00ffffff" />
                                <Setter Property="BorderBrush" Value="#00ffffff" />
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                            </Style>
                            <Style TargetType="ComboBox">
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                                <Setter Property="Height" Value="18" />
                            </Style>
                        </Grid.Resources>
                        <TextBox Text="{Binding CharacterName, UpdateSourceTrigger=PropertyChanged}" FontSize="19" Width="215" Margin="42,68,0,0" />
                        <TextBox Text="{Binding Age, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="315,53,0,0" />
                        <TextBox Text="{Binding Height, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="450,53,0,0" />
                        <TextBox Text="{Binding Weight, UpdateSourceTrigger=PropertyChanged}" Width="91" Margin="585,53,0,0" />
                        <TextBox Text="{Binding Eyes, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="315,84,0,0"/>
                        <TextBox Text="{Binding Skin, UpdateSourceTrigger=PropertyChanged}" Width="130" Margin="450,84,0,0" />
                        <TextBox Text="{Binding Hair, UpdateSourceTrigger=PropertyChanged}" Width="91" Margin="585,84,0,0" />
                        <Button Style="{StaticResource ImageButton}" Height="270" Width="200" Margin="30,146,0,0" Command="{Binding LoadPortraitCommand}">
                            <Image Source="{Binding PortraitSource}" />
                        </Button>
                        <TextBox Text="{Binding OrganizationName, UpdateSourceTrigger=PropertyChanged}" Margin="500,176,0,0" Width="168" TextAlignment="Center" />
                        <Button Style="{StaticResource ImageButton}" Height="136" Width="168" Margin="500,196,0,0" Command="{Binding LoadSymbolCommand}">
                            <Image Source="{Binding SymbolSource}" />
                        </Button>
                        <Grid>
                            <Grid.Resources>
                                <Style TargetType="TextBox">
                                    <Setter Property="Padding" Value="5" />
                                    <Setter Property="TextBlock.LineHeight" Value="12" />
                                    <Setter Property="TextBlock.LineStackingStrategy" Value="BlockLineHeight" />
                                    <Setter Property="Background">
                                        <Setter.Value>
                                            <DrawingBrush TileMode="Tile" Viewbox="0,0.5,1,1" Stretch="None" Viewport="0,0,12,12" ViewportUnits="Absolute">
                                                <DrawingBrush.Drawing>
                                                    <GeometryDrawing>
                                                        <GeometryDrawing.Pen>
                                                            <Pen Brush="Gray" Thickness="0.25"/>
                                                        </GeometryDrawing.Pen>
                                                        <GeometryDrawing.Geometry>
                                                            <LineGeometry StartPoint="0,0" EndPoint="12,0"/>
                                                        </GeometryDrawing.Geometry>
                                                    </GeometryDrawing>
                                                </DrawingBrush.Drawing>
                                            </DrawingBrush>
                                        </Setter.Value>
                                    </Setter>
                                    <Setter Property="BorderBrush" Value="#00ffffff" />
                                    <Setter Property="HorizontalAlignment" Value="Left" />
                                    <Setter Property="VerticalAlignment" Value="Top" />
                                    <Setter Property="AcceptsReturn" Value="True" />
                                    <Setter Property="TextWrapping" Value="Wrap" />
                                    <Setter Property="TextAlignment" Value="Left" />
                                    <Setter Property="FontSize" Value="10" />
                                </Style>
                            </Grid.Resources>
                            <TextBox Text="{Binding CharacterBackstory, UpdateSourceTrigger=PropertyChanged}" Width="210" Height="454" Margin="25,451,0,0" />
                            <TextBox Text="{Binding AlliesAndOrganizations1, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="270" Margin="253,144,0,0" />
                            <TextBox Text="{Binding AlliesAndOrganizations2, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="55" Margin="477,359,0,0" />
                            <TextBox Text="{Binding AdditionalFeaturesAndTraits1, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="254" Margin="253,439,0,0" />
                            <TextBox Text="{Binding AdditionalFeaturesAndTraits2, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="254" Margin="477,439,0,0" />
                            <TextBox Text="{Binding Treasure1, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="191" Margin="253,715,0,0" />
                            <TextBox Text="{Binding Treasure2, UpdateSourceTrigger=PropertyChanged}" Width="216" Height="191" Margin="477,715,0,0" />
                        </Grid>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Spellcasting">
                <Grid>
                    <ContentPresenter Height="941" Width="720" VerticalAlignment="Top" HorizontalAlignment="Left" Content="{StaticResource SpellcastingBackground}" />
                    <Grid>
                        <Grid.Resources>
                            <Style TargetType="TextBlock">
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                                <Setter Property="TextAlignment" Value="Center" />
                                <Setter Property="FontSize" Value="18" />
                            </Style>
                            <Style TargetType="TextBox">
                                <Setter Property="Background" Value="#00ffffff" />
                                <Setter Property="BorderBrush" Value="#00000000" />
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                                <Setter Property="TextAlignment" Value="Center" />
                                <Setter Property="FontSize" Value="14" />
                            </Style>
                            <Style TargetType="ComboBox">
                                <Setter Property="HorizontalAlignment" Value="Left" />
                                <Setter Property="VerticalAlignment" Value="Top" />
                                <Setter Property="Template" Value="{StaticResource ComboBoxControlTemplate}" />
                            </Style>
                        </Grid.Resources>
